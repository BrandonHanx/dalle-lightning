--zone ${ZONE} --project ${PROJECT_ID} --worker=all \
  --command "gcsfuse lgaivision-coco-eu vqgan/data"

gcloud alpha compute tpus tpu-vm ssh ${TPU_NAME} \
--zone ${ZONE} --project ${PROJECT_ID} --worker=all \
  --command "gcsfuse lgai-vision-tpu-results vqgan/results"

